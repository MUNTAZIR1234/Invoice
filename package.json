
{
  "name": "queens-chambers-manager",
  "version": "1.0.0",
  "description": "Property Management & Invoicing Software",
  "main": "electron-main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.queenschambers.manager",
    "win": {
      "target": "nsis",
      "icon": "icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "devDependencies": {
    "electron": "^25.0.0",
    "electron-builder": "^24.4.0"
  }
}
